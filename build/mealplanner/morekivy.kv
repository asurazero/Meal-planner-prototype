<HelpScreen>
    on_enter:(root.displaytxt1())
    textview:textview
    orientation:"vertical"
    size:.1,.1
    ScrollView:
        canvas.before:
            Color:
                rgba:(0,0,0,1)
            Rectangle:
                pos:self.pos
                size:self.size
        Label:
            markup:True
            id:textview
            background_color:"#00FFCE"
            text:""""""
            bold:True
            #font_size:11
            text_size:self.width,None
            size_hint_y:None
            height: max(self.texture_size[1],self.parent.height)
            size_x:.5
            do_scroll_y:True
            do_scroll_x:True
            valign:"top"
            halign:"center"
    Button:
        size_hint:(1.,0.1)
        #post_hint:(1,1)
        text:"Back"
        background_color:"#00FFCE"
        on_press:
            root.manager.current="StartScreen"

<WebScraperConfig>
