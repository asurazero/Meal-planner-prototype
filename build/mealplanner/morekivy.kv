LoginWindow:

<Label@Label+BackgroundColor>
    background_color:"#000000"

<LoginWindow>:
    on_enter:(root.trigger_libraries())
    BoxLayout:
        spacing:20
        padding:100
        orientation:"vertical"
        size:root.width,root.height
        canvas.before:
            Color:
                rgba:(1,1,1,1)
            Rectangle:
                source:"fnv1.jpg"
                pos:self.pos
                size:self.size
        Button:
            size_hint:(1.,.2)
            background_color:"#00FFCE"
            font_size_hint:(0.1,0.1)
            text:"View Meal Plans"
            background_color:"#00FFCE"
            on_press:
                root.manager.current="MealCheck"
                #allows change of transition
                #root.manager.transition=


        Button:
            size_hint:(1.,.2)
            background_color:"#00FFCE"
            font_size_hint:(0.1,0.1)
            text:"Generate New Meal Plan"
            background_color:"#00FFCE"
            on_press:
                root.manager.current="Day Select"
                #allows change of transition
                #root.manager.transition=
        Button:
            size_hint:(1.,.2)
            background_color:"#00FFCE"
            font_size_hint:(0.1,0.1)
            text:"Add New Meal Item"
            background_color:"#00FFCE"
            on_press:
                root.manager.current="AddNewItem"
                #allows change of transition
                #root.manager.transition=
        Button:
            size_hint:(1.,.2)
            background_color:"#00FFCE"
            font_size_hint:(0.1,0.1)
            text:"Pull New Meals From Internet"
            background_color:"#00FFCE"
            on_press:
                root.manager.current="WebScraperConfig"
                #allows change of transition
                #root.manager.transition=
        Button:
            size_hint:(1.,.2)
            background_color:"#00FFCE"
            font_size_hint:(0.1,0.1)
            text:"Tools"
            background_color:"#00FFCE"
            on_press:
                root.manager.current="Tools"
                #allows change of transition
                #root.manager.transition=
        Button:
            size_hint:(1.,.2)
            background_color:"#00FFCE"
            font_size_hint:(0.1,0.1)
            text:"Help"
            background_color:"#00FFCE"
            on_press:
                root.manager.current="HelpScreen"
                #allows change of transition
                #root.manager.transition=
