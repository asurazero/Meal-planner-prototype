        Label:
            text:"How Many New Meals To Find?"
            bold:True
            background_color:"#00FFCE"
            font_size_hint:(.1,.1)
            size_hint:(1.,1.)
            outline_width: 5
            outline_colour: (0, 0, 0, 1)
        TextInput:
            id:scrape_amt
            multiline:False
            input_filter:"int"
            size_hint:(.5,0.8)
            pos_hint: {"center_x": .5, "center_y": .5}
        Button:
            size_hint:(1,0.8)
            text:"Confirm"
            background_color:"#00FFCE"
            on_press:
                root.scraper_amount()
                #allows change of transition

<WebScraper>
    on_enter:
        root.webscrape_search()
    progress_bar:progress_bar

    MDFloatLayout:
        canvas.before:
            Color:
                rgba:(1,1,1,1)
            Rectangle:
                source:"fnv3.jpg"
                pos:self.pos
                size:self.size
        Label:
            text:"Searching"
            bold:True
            font_size_hint:(.1,.1)
            size_hint:(.8,.2)
            pos_hint: {"center_x": .5, "center_y": .5}
            canvas.before:
                Color:
                    rgba:0,0,0,1
                Rectangle:
                    size:self.size
                    pos:self.pos
        MDProgressBar
            id:progress_bar
            min:0
            max:50
            value: 1
            #size_hint: .1, None
            size_hint_x:.8
            size_hint_y:.2
            height:dp(48)
            pos_hint: {"center_x": .5, "center_y": .5}
            color:"#00FFCE"

