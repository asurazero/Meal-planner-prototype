#:import Factory kivy.factory.Factory
#popup area
<EntreeLimitPop@Popup>
    size_hint: 1, 0.3
    auto_dismiss:False
    title:"Invalid Range"
    FloatLayout:
        spacing:10
        padding:100
        orientation:"vertical"
        Label:
            text:"Can only enter a number between 7 and 30"
            bold:True
            font_size_hint:(.1,.1)
            size_hint:(1,1)
            pos_hint: {"center_x": .5, "center_y": .8}
        Button:
            size_hint:(.5,0.3)
            pos_hint: {"center_x": .5, "center_y":.5 }
            #post_hint:(15,10)
            text:"Ok"
            background_color:"#00FFCE"
            on_release:root.dismiss()
<SidesEntreesPop@Popup>
    size_hint: 1, 0.3
    auto_dismiss:False
    title:"Invalid Selection"
    FloatLayout:
        spacing:10
        padding:100
        orientation:"vertical"
        Label:
            text:"You can have up to 1 entree and 3 sides"
            bold:True
            font_size_hint:(.1,.1)
            size_hint:(1,1)
            pos_hint: {"center_x": .5, "center_y": .8}
        Button:
            size_hint:(.5,0.3)
            pos_hint: {"center_x": .5, "center_y":.5 }
            #post_hint:(15,10)
            text:"Ok"
            background_color:"#00FFCE"
            on_release:root.dismiss()
<WebScrapeLimitPop@Popup>
