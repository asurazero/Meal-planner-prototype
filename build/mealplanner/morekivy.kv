<WebScraperQty>
    scrape_amt:scrape_amt
    BoxLayout:
        spacing:200
        padding:50
        orientation:"vertical"
        size:root.width,root.height
        canvas.before:
            Color:
                rgba:(1,1,1,1)
            Rectangle:
                source:"fnv4.jpg"
                pos:self.pos
